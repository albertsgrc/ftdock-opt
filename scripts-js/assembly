#! /usr/bin/env node
// Generated by CoffeeScript 1.10.0
(function() {
  var EXECUTABLES_DIR, OPTIM, aux, e, err, error, error1, forward, program, ref;

  e = require('child_process').execSync;

  ref = require('./common'), EXECUTABLES_DIR = ref.EXECUTABLES_DIR, OPTIM = ref.OPTIM;

  process.argv = process.argv.slice(2);

  try {
    e("./compile assembly", {
      stdio: 'inherit'
    });
  } catch (error) {
    err = error;
    process.exit(1);
  }

  program = (process.argv[0] != null) && process.argv[0][0] !== '-' ? (aux = process.argv[0], process.argv = process.argv.slice(1), aux) : "ftdock";

  forward = process.argv.join(" ");

  process.chdir(EXECUTABLES_DIR);

  try {
    e(OPTIM + " assembly " + program + ".3g -F -p " + forward, {
      stdio: 'inherit'
    });
  } catch (error1) {
    err = error1;
  }

}).call(this);
