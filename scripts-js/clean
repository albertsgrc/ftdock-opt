#! /usr/bin/env node
// Generated by CoffeeScript 1.10.0
(function() {
  var EXECUTABLES_DIR, OPTIM, e, err, error, forward, ref;

  e = require('child_process').execSync;

  ref = require('./common'), EXECUTABLES_DIR = ref.EXECUTABLES_DIR, OPTIM = ref.OPTIM;

  process.argv = process.argv.slice(2);

  process.chdir(EXECUTABLES_DIR);

  forward = process.argv.join(" ");

  try {
    e(OPTIM + " clean " + forward, {
      stdio: 'inherit'
    });
    e("rm -f scratch*");
  } catch (error) {
    err = error;
  }

}).call(this);
